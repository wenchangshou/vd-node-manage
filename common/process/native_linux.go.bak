func StartProcessAsCurrentUser(appPath, cmdLine, workDir string, backstage bool) (uint32, error) {
	env := os.Environ()
	procAttr := &os.ProcAttr{
		Env: env,
		Files: []*os.File{
			os.Stdin,
			os.Stdout,
			os.Stderr,
		},
	}
	strings.split(cmdLine, " ")
	pid, err := os.StartProcess(appPath, cmdLine)
	fmt.Println("procAttr", procAttr)
	return 0, nil

}
